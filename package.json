{
  "name": "mosaic-gui",
  "version": "1.0.0",
  "description": "Mosaic Generator GUI",
  "scripts": {
    "tauri": "tauri",
    "dev": "GSK_RENDERER=ngl WEBKIT_DISABLE_DMABUF_RENDERER=1 tauri dev",
    "build": "NO_STRIP=true GSK_RENDERER=ngl WEBKIT_DISABLE_DMABUF_RENDERER=1 tauri build",
    "vite": "vite",
    "vite:build": "vite build",
    "test": "cargo test --manifest-path src-tauri/Cargo.toml",
    "typecheck": "cargo check --manifest-path src-tauri/Cargo.toml --all-targets",
    "lint": "cargo fmt --manifest-path src-tauri/Cargo.toml --all -- --check && cargo clippy --manifest-path src-tauri/Cargo.toml --all-targets -- -D warnings",
    "qa": "npm run lint && npm run typecheck && npm run test"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@tauri-apps/cli": "^2.0.0",
    "vite": "^7.3.0"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.9.1",
    "@tauri-apps/plugin-dialog": "^2.4.2"
  }
}
